@using Microsoft.AspNetCore.Identity
@model IEnumerable<MindClinic.Models.Appointment>
@inject SignInManager<User> signInManager;

@{
    ViewData["Title"] = "getslots";
}

<h1>getslots</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>

<table  class="table" >
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Time)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td >
                    @Html.DisplayFor(modelItem => item.Time)
                </td>
                <td>
                    <a  asp-route-DoctorId="@Context.Request.RouteValues["id"]" asp-route-a="@item.Time" asp-controller="Appointments" asp-action="TimeSelected" >Select</a> 

                </td>
            </tr>
        }
    </tbody>
</table>
